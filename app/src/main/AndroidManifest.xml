        android:name="android.permission.WRITE_SETTINGS"
        tools:ignore="ProtectedPermissions" />
    <application
        android:name="com.supercom.puretrack.util.application.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        android:largeHeap="true"
        tools:targetApi="m">

        <meta-data android:name="com.samsung.knoxlicense.permissions" android:value="true"/>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />



        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.PhoneCallReceiver">
            <intent-filter android:priority="100">
                <action android:name="android.intent.action.REBOOT" />
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.QUICKBOOT_POWERON" />
                <action android:name="android.intent.action.ACTION_SHUTDOWN" />
                <action android:name="android.intent.action.PACKAGE_FIRST_LAUNCH" />
                <action android:name="android.intent.action.PACKAGE_RESTARTED" />
                <action android:name="android.intent.action.QUICKBOOT_POWERON" />
                <action android:name="android.intent.action.PACKAGE_RESTARTED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
                <action android:name="android.intent.action.CLOSE_SYSTEM_DIALOGS" />
                <action android:name="android.intent.action.CLOSE_SYSTEM_DIALOGS" />
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />
                <action android:name="android.intent.action.PACKAGE_RESTARTED" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.SimChangedReceiver">
            <intent-filter>
                <action android:name="android.intent.action.SIM_STATE_CHANGED" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.IncomingCallReceiver">
            <intent-filter android:priority="100">
                <action android:name="android.intent.action.PHONE_STATE" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.OutgoingCallReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" />
                <action android:name="android.intent.action.NEW_OUTGOING_CALL" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.WakingAlarmBroadcastReceiver" />
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.WatchdogReceiver" />
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.NewDayStartReceiver" />
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.UpdateUIReceiver" />
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.NetworkCycleReceiver" />
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.LocationPointReceiver" />
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.WakingAlarmBroadcastReceiver" />
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.SmsReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.NetworkChangeReceiver">
            <intent-filter>
                <action android:name="android.intent.action.AIRPLANE_MODE" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true"
            android:name=".service.heart_beat.TurnOnReceiver">
            <intent-filter>
                <action android:name="ACTION_TurnOnScreen" />
            </intent-filter>
        </receiver>

        <receiver android:exported="true"
            android:name="com.supercom.puretrack.data.broadcast_receiver.KnoxAdminReceiver"
            android:permission="android.permission.BIND_DEVICE_ADMIN">
            <meta-data
                android:name="android.app.device_admin"
                android:resource="@xml/device_admin_receiver" />

            <intent-filter>
                <action android:name="android.app.action.DEVICE_ADMIN_ENABLED" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true"
            android:name="com.supercom.puretrack.data.broadcast_receiver.KnoxLicenseReceiver"
            android:enabled="true">
            <intent-filter>
                <action android:name="edm.intent.action.license.status" />
            </intent-filter>
        </receiver>
        <receiver android:exported="true" android:name="com.supercom.puretrack.data.broadcast_receiver.TimeChangedReceiver">
            <intent-filter>
                <action android:name="android.intent.action.TIME_SET" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.supercom.puretrack.data.broadcast_receiver.RestartBackgroundReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="restartservice" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.supercom.puretrack.ui.activity.TurnOnScreenActivity"
            android:exported="true" />

        <activity android:exported="true"
            android:name="com.supercom.puretrack.ui.activity.MainActivity"
            android:configChanges="locale|layoutDirection"
            android:excludeFromRecents="true"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.settings.SETTINGS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.settings.WIRELESS_SETTINGS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.DIAL" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="tel" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.DIAL" />
                <action android:name="android.intent.action.CALL" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:exported="true" android:name="com.supercom.puretrack.ui.activity.CheckPermissionActivity"/>
        <activity android:exported="true"
            android:name="com.supercom.puretrack.ui.activity.LauncherActivity"
            android:configChanges="locale|layoutDirection"
            android:excludeFromRecents="true"
            android:label="Simple Launcher Home"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:stateNotNeeded="true"
            android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.HOME" />
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="android.intent.action.ACTION_REQUEST_SHUTDOWN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.supercom.puretrack.ui.enrollment.activity.EnrollmentActivity"
            android:configChanges="locale|layoutDirection"
            android:excludeFromRecents="true"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.supercom.puretrack.ui.activity.EnterPasswordActivity"
            android:configChanges="locale|layoutDirection"
            android:excludeFromRecents="true"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:taskAffinity=""
            android:theme="@android:style/Theme.Translucent"
            tools:ignore="LockedOrientationActivity" />

        <service android:exported="true"
            android:name=".service.DeviceConfigurationAccessibilityService"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>
            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/device_settings_configuration_service_actions" />
        </service>
        <service
            android:name=".cycle.CycleService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".service.heart_beat.HeartBeatServiceJava2"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.supercom.puretrack.data.service.WatchdogService"
            android:enabled="true"
            android:process=":watchdog_process" />

        <service android:name="com.supercom.puretrack.data.service.ActivityRecognizedService" />

        <activity
            android:name="com.supercom.puretrack.ui.lockscreen.LockScreenActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen"
            tools:ignore="LockedOrientationActivity" />

        <service android:name="com.supercom.puretrack.data.service.ActivityRecognizedService" />
        <service android:name="com.supercom.puretrack.ui.lockscreen.LockScreenService" />

        <receiver android:exported="true"
            android:name="com.supercom.puretrack.ui.lockscreen.LockScreenReceiver"
            android:permission="android.permission.RECEIVE_BOOT_COMPLETED"
            android:enabled="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.supercom.puretrack.data.service.LocationService"
            android:enabled="true"
            android:exported="true" />

        <service
            android:name="com.supercom.puretrack.data.service.ToolbarInfoUtilsService"
            android:enabled="true"
            android:exported="false" />

